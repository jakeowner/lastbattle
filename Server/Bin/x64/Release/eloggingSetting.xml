<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ELoggingCfg xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<!--  是否输出到屏幕(0:禁止)-->
	<ConsoleDump>1</ConsoleDump>
	<!--  是否输出到文件(0:禁止)-->
	<FileDump>1</FileDump>
	<!--  缓存多少个日志才输出文件(0:禁止)-->
	<BufferDumpToFileNum>10</BufferDumpToFileNum>
	<!--  最长多长时间（毫秒）输出文件-->
	<BufferDumpToFileMaxTime>10</BufferDumpToFileMaxTime>
	<!--  是否输出Info(0:禁止)-->
	<IfDumpInfo>1</IfDumpInfo>
	<!--  是否输出Debug(0:禁止)-->
	<IfDumpDebug>1</IfDumpDebug>
	<!--  是否输出SpecialDebug(0:禁止)-->
	<IfDumpSpecialDebug>1</IfDumpSpecialDebug>
	<!--  是否输出Error(0:禁止)-->
	<IfDumpError>1</IfDumpError>
	<!--  是否断言弹框(0:禁止)-->
	<IfMsgBoxAssert>1</IfMsgBoxAssert>
	<!--  是否输出Warning(0:禁止)-->
	<IfDumpWarning>1</IfDumpWarning>
	<!--  是否重用当天日志文件(0:否)-->
	<IfReuseCurDayLogging>1</IfReuseCurDayLogging>
</ELoggingCfg>